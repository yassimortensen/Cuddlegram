<h2> <a href = "<%= @user.instagram_account_link%>"> <%= @user.instagram_handle%> </a> </h2>

<br>
<br>
CUDDLES
<ul>
  <% @user.pets.each do |pet| %>
  <li> <%= link_to pet.instagram_handle, pet_path(pet) %> </li>
  <% end %>
</ul>

GALLERY
<ul>
  <% @user.pets.each do |pet| %>
    <% pet.photos.each do |photo| %>
      <li> <%= link_to image_tag(photo.url), photo_path(photo) %>
    <% end %>
  <% end %>
  </ul
